<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_test_attachment_form"
                model="ir.ui.view">
            <field name="name">test.attachment.form</field>
            <field name="model">test.attachment</field>
            <field name="arch"
                    type="xml">
                <form string="ModelTitle">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="url"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_test_attachment_tree"
                model="ir.ui.view">
            <field name="name">test.attachment.tree</field>
            <field name="model">test.attachment</field>
            <field name="arch"
                    type="xml">
                <tree>
                    <field name="name"/>
                    <field name="url"/>
                </tree>
            </field>
        </record>

